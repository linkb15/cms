{
  "name": "@linkbcms/core-config",
  "version": "0.0.0",
  "private": true,
  "description": "A simple example library built by tsdown",
  "type": "module",
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./beta": {
      "types": "./dist/beta.d.ts",
      "import": "./dist/beta.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "tsdown": "latest"
  }
}
