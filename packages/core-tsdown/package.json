{
  "name": "@linkbcms/core-tsdown",
  "version": "0.0.1",
  "scripts": {
    "generate-paraglide": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "build": "pnpm generate-paraglide && tsdown",
    "check": "biome check --write",
    "dev": "tsdown --watch",
    "format": "biome format --write",
    "machine-translate": "inlang machine translate --project project.inlang"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./styles": "./dist/styles.css",
    "./styles/non-compiled": "./src/styles.css"
  },
  "files": ["dist", "package.json", "README.md", "LICENSE"],
  "peerDependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@inlang/cli": "^3.0.12",
    "@inlang/paraglide-js": "2.2.0",
    "@legendapp/state": "3.0.0-beta.30",
    "@linkbcms/ui": "workspace:*",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-form": "^1.15.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.130.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-cli": "^1.130.2",
    "@tanstack/router-plugin": "^1.130.2",
    "@types/node": "^22.16.5",
    "@types/pluralize": "^0.0.33",
    "@types/react": "19.1.1",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "date-fns": "^4.1.0",
    "lucide-react": "0.476.0",
    "pluralize": "^8.0.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "react-number-format": "^5.4.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^4.1.11",
    "tsdown": "latest",
    "typescript": "^5.8.3",
    "wouter": "^3.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
